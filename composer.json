{
    "name": "vendor/bestiablast",
    "description": "BestiaBlast is a fork of ViroBlast reengineered by a real computer scientist",
    "authors": [
        {
            "name": "shady",
            "email": "snipershady@gmail.com"
        }
    ],
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-apcu": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "google/recaptcha": "^1.2",
        "snipershady/ipapigeolocation": "^1.0",
        "snipershady/pwned-password-check": "^1.0",
        "snipershady/ratelimiter": "^1.0",
        "snipershady/securecipher": "^1.0",
        "snipershady/typeidentifier": "^1.0",
        "symfony/yaml": "^6.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "classmap": ["src"],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "*",
        "rector/rector": "^0.15"
    }
}
